{
    "name": "Flowing Bells",
    "description": "Ring the bell.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["alarms","storage"],
    "options_page": "options.html",

    "action": {
    "default_popup": "options.html"
    },
    "icons": {
